{
  "short_name": "Schedule App",
  "name": "Schedule App for MIREA",
  "manifest_version": 3,
  "version": "1.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open"
  },
  "permissions": [
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}